a#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n,test;
	cin>>test;
	while(test--)
	{
	cin>>n;
	switch(n)
	{
		case 1: 
			if (n==1);
			cout<<"6";
			break;
		case 2: 
		if (n==2);
		cout<<"5";
		break;
		case 3: if (n==3);
		cout<<"4";
		break;
		case 4: if (n==4);
		cout<<"3";
		break;
		case 5: if (n==5);
		cout<<"2";
		break;
		case 6: if (n==6);
		cout<<"1";
		break;
		default: cout<<"not defind";
	}	
}
	return 0;	
}
